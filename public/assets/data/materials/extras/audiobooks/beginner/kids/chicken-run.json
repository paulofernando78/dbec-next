    {
      "paragraphSections": [
        {
          "imgSrc": "/materials/extras/audiobooks/starter/kids/chicken-run/cover.png",
          "imgAlt": "Cover illustration",
          "imgPosition": "left",
          "paragraphs": [
            {
              "enText": "Chicken Run is about Ginger and her chicken friends escaping a farm where they're turned into pies. With the help of Rocky, a rooster, they plan to escape to freedom!"
            }
          ]
        }
      ]
    },
    {
      "imgSrc": "/materials/extras/audiobooks/starter/kids/chicken-run/characters.png",
      "imgAlt": "Character illustration"
    },
    {
      "id": "0cd964a9-0548-41d2-a76b-f4fc7cf19d8e",
