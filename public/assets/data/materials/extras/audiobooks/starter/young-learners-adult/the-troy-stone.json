              "options": [
                {
                  "id": "b4e7f1c5-8d2e-4b91-b7c9-e2a3b7c4f5d6",
                  "label": "They can't find a stone for the second eye.",
                  "isCorrect": true
                },
                {
                  "id": "c5e4a3b7-1d2f-4b91-b7c9-d2f9e4c9b7a9",
