  "lessons": [
    {
      "title": "Summary",
      "bgColor": "black",
      "textColor": "white"
    },
    {
      "paragraphSections": [
        {
          "imgSrc": "/extras/skills/listening/audiobooks/beginner/general/the-pearl-girl/cover.jpg",
          "imgAlt": "Cover illustration",
          "imgPosition": "left",
          "paragraphs": [
            {
              "textEn": "Kate Grant comes from Canada. She is visiting Europe with her mother and father. One evening she sees two men in a museum. They are taking a very famous picture, Girl with a Pearl Earring. What can Kate do?"
            }
          ]
        }
      ]
    },
